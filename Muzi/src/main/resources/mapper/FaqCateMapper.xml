<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<!-- mapper 태그에 namespace 등록하기 -->
<mapper namespace="toy2.Faq.Dao.FaqCateDao">

    <select id = "selectAllNames" resultType="map">
        SELECT cate_no, cate_name FROM faq_cate
        ORDER BY cate_no ASC
    </select>

</mapper>